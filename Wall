import java.awt.Rectangle;


public class Wall 
{

    private int xPos;
    private int yPos;
    
	private Rectangle rekt;
	
	public Wall(int x, int y)
	{				
	 
	
	 rekt = new Rectangle(x, y, 25, 25);
	
	 xPos = x;
	 yPos = y;
	 
	}


  
	
	
	public Rectangle getRekt()
	{
		return rekt;				
	}


	public int getX()
	{
		return xPos;
	}

	public int getY()
	{
		return yPos;
	}


}
