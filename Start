import java.awt.*;
import java.lang.*;

//This will be the start screen
//Has a display method

public class Start
{
  private String title= "Infinity Maze";
  private String authors= "By: Justin, Michael, Josh, and Jacob";
  
  public Start()
  {
    setBackground(Color.WHITE);
		setVisible(true);
  }
  public void run()
  {
    graphToBack.setColor(Color.BLACK);
		graphToBack.drawString(title, 400, 295);      //first int is x direction
		graphToBack.drawString(authors, 400, 305);
  }
}

